{
  "name": "harisenin.webapp",
  "version": "5.1.0",
  "private": true,
  "engines": {
    "node": ">=10.15"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "start:prod": "node --optimize_for_size --max_old_space_size=920 scripts/startProd.js",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.{js,json,css,md}\"",
    "docker:build": "docker build --rm -t cullenjett/react-ssr-boilerplate .",
    "docker:start": "docker run --rm -it -p 3000:3000 cullenjett/react-ssr-boilerplate",
    "docker": "npm run docker:build && npm run docker:start"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint"
    ],
    "*.{js,json,css,md}": [
      "npm run format",
      "git add"
    ]
  },
  "dependencies": {
